# Uncomment the next line to define a global platform for your project
 platform :ios, '14.0'

def main
  pod 'Alamofire', '~> 5.2'
  pod 'SwiftyJSON', '~> 4.0'
end

target 'Benuse (iOS)' do
  main
  use_frameworks!
end

target 'Benuse (macOS)' do
  main
  use_frameworks!
end

target 'BenuseExtension' do
  main
  use_frameworks!
end
